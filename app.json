{
  "expo": {
    "name": "Story 100",
    "slug": "story100",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package":  "com.lzayyakoko.story100",
      "adaptiveIcon": {
        "foregroundImage": "./assets/wbook.png",
        "backgroundColor": "#fcd5ce"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}